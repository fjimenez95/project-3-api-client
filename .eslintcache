unt":0,"usedDeprecatedRules":"40"},{"filePath":"70","messages":"71","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"72","usedDeprecatedRules":"40"},{"filePath":"73","messages":"74","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"40"},{"filePath":"75","messages":"76","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"40"},{"filePath":"77","messages":"78","errorCount":0,"warningCount":3,"fixableErrorCount":0,"fixableWarningCount":0,"source":"79","usedDeprecatedRules":"40"},"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/index.js",[],["80","81"],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/reportWebVitals.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/App.js",["82"],"import { getUser, logout } from './services/userService';\nimport { useState, Redirect, useEffect } from 'react';\n\nimport './App.css';\nimport Footer from './components/Footer/Footer';\nimport { Route, Switch, withRouter, useHistory } from 'react-router-dom';\n// YOU CAN CHANGE THE FOOTER / HEADER FILES TO INDEX.JS AND YOU DON'T HAVE TO DO THE FINAL PREFIX /FOOTER BECAUSE SYSTEM WILL ALREADY KNOW TO LOOK FOR INDEX.JS\nimport Header from './components/Header/Header';\n\nimport HomePage from './pages/HomePage';\nimport DashboardPage from './pages/DashboardPage';\nimport LoginPage from './pages/LoginPage';\nimport SignupPage from './pages/SignupPage';\nimport TodoPage from './pages/TodoPage';\nimport EditTodoPage from './pages/EditTodoPage';\nimport CreateTodoPage from './pages/CreateTodoPage';\nimport { getTodos, createTodo } from './services/todoService';\n\n// 1. Create four page components, Login, Signup, Dashboard, Home\n// 2. Define as a function component and export them\n// 3. Have return a div with the text content of Login, Signup ... etc\n// 4. Import them into App.js\n// 5. Import Switch and Route components from React Router and then define the various routes\n\nfunction App(props) {\n\n  const history = useHistory();\n\n  const [ userState, setUserState ] = useState({\n    user: getUser(),\n  });\n  const [ userTodos, setUserTodos ] = useState([]);\n\n  useEffect(() => {\n    if(userState.user) {\n      fetchItems();\n    }\n  }, [userState.user])  \n  \n  async function fetchItems() {\n      const todos = await getTodos();\n      console.log('TODOS', todos);\n      setUserTodos(todos);\n  }\n\n  function handleSignupOrLogin() {\n    setUserState({\n      user: getUser(),\n    })\n  }\n\n  function handleLogout() {\n    logout();\n    setUserState({ user: null });\n    // CAN USE THIS CAUSE OF WITHROUTER AND ADDING PROPS TO APP() ^\n    props.history.push('/');\n  }\n\n  async function handleCreateSubmit(data) {\n    data.user = userState.user._id;\n    const response = await createTodo(data);\n    console.log('RESPONSE FROM POST', response);\n    setUserTodos(response);\n    history.push('/todos');\n  }\n\n  function handleEditSubmitResponse(response) {\n    setUserTodos(response);\n  }\n  // const onSubmit = (data) => {\n  //   alert(JSON.stringify(data));\n  //   async function createANewTodo() {\n  //       const todos = await createTodo(data);\n  //       console.log('TODOS', todos);\n  //       setUserTodos(todos);\n  //   }\n  // };\n\n  function deleteHandler() {\n    \n  }\n\n  return (\n    <div className=\"App\">\n      <Header handleLogout={handleLogout} user={userState.user} />\n      <main>\n        <Switch>\n          <Route exact path='/' render={(props) =>\n            <HomePage\n            />\n          } />\n          \n          <Route exact path='/dashboard' render={(props) =>\n              // HOW TO PROTECT THIS PAGE FROM USERS NOT LOGGED IN\n              userState.user ?\n              <DashboardPage\n              />\n              :\n              // USING THIS REDIRECT INSTEAD OF JUST LOGINPAGE IT WILL ALLOW YOU TO USE REFERRER TO BRING YOU BACK TO PAGE YOU WERE TRYING TO REACH AKA DASHBOARD\n              <Redirect to='/login' />\n            } />\n\n          <Route exact path='/login' render={(props) =>\n            <LoginPage\n              {...props}\n              handleSignupOrLogin={handleSignupOrLogin}\n            />\n          } />\n\n          <Route exact path='/signup' render={(props) =>\n            <SignupPage\n              {...props}\n              handleSignupOrLogin={handleSignupOrLogin}\n            />\n          } />\n\n          <Route exact path='/todos' render={(props) =>\n            <TodoPage \n              {...props}\n              todos={userTodos}\n              deleteHandler={deleteHandler}\n              setUserTodos={setUserTodos}\n              />\n          } />\n\n          <Route path='/todos/edit/:id' render={(props) =>\n            <EditTodoPage \n              {...props}\n              todos={userState.todos}\n              setUserTodos={setUserTodos}\n              />\n          } />\n\n          <Route path='/todos/create' render={(props) =>\n            <CreateTodoPage \n              {...props}\n              todos={userState.todos}\n              onCreateSubmit={handleCreateSubmit}\n              />\n          } />  \n        </Switch>\n      </main>\n      <Footer />\n    </div>\n  );\n}\n\nexport default withRouter(App);\n","/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/components/Footer/Footer.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/components/Header/Header.js",["83","84"],"import { Link } from 'react-router-dom';\nimport styles from './Header.module.css';\nimport logo from './todoslogo.png'\nimport { useState } from 'react';\n\nconst Header = (props) => {\n    const [color, changeColor] = useState(\"#282c34\");\n\n    return (\n        <header className={styles.Header}>\n            <Link to='/'>\n                <img className={styles.logo} src={logo} alt=\"Logo\" />\n            </Link>\n            <nav>\n                <ul>\n                    {\n                        props.user ?\n                        <>\n                            <li><Link className={styles.headerbutton} to='/' onClick={props.handleLogout}>logout</Link></li>\n                            {/* <li><Link className={styles.headerbutton} to='/' onClick={() => changeColor(\"#282c34\")}>dashboard</Link></li> */}\n                            <li><Link className={styles.headerbutton} to='/todos'>my todos</Link></li>\n                            <li><Link className={styles.headerbutton} to='/todos/create'>create new todo</Link></li>\n                        </>\n                        :\n                        <>\n                            <li><Link className={styles.headerbutton} to='/login'>login</Link></li>\n                            <li><Link className={styles.headerbutton} to='/signup'>sign up</Link></li>\n                        </>\n                    }\n                </ul>\n            </nav>\n        </header>\n    )\n}\n\nexport default Header;","/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/pages/DashboardPage/index.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/pages/HomePage/index.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/pages/LoginPage/index.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/pages/SignupPage/index.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/components/SignupForm/SignupForm.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/services/userService.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/services/tokenService.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/services/todoService.js",["85"],"// const getTodos = () => fetch(\"http://localhost:3002/todos\").then(res => res.json).catch((error) => console.log(error));\nimport TodoForm from '../components/TodoForm';\nimport { getToken } from './tokenService';\n\n\nexport function getTodos() {\n    return fetch('https://git.heroku.com/todos4teams.git/todos', {\n    method: 'GET',\n    headers: \n        {\n          'Authorization': 'Bearer ' + getToken(),\n        }\n    })\n    .then(res => res.json())\n    .catch(err => console.log(err))\n}\n\nexport function createTodo(todo) {\n  return fetch('https://git.heroku.com/todos4teams.git/todos/create', {\n    method: 'POST',\n    headers: \n        {\n          'Content-Type': 'Application/json',\n          'Authorization': 'Bearer ' + getToken(),\n        },\n    body: JSON.stringify(todo),\n    })\n    .then(res => res.json())\n    .catch(err => console.log(err))\n}\n\nexport function getTodo(id) {\n  return fetch(`https://git.heroku.com/todos4teams.git/todos/${id}`, {\n    method: 'GET',\n    headers: \n        {\n          'Authorization': 'Bearer ' + getToken(),\n        }\n    })\n    .then(res => res.json())\n    .catch(err => console.log(err))\n}\n\nexport function updateTodo(todo, id) {\n  return fetch(`https://git.heroku.com/todos4teams.git/todos/${id}`, {\n    method: 'POST',\n    headers: \n        {\n          'Content-Type': 'Application/json',\n          'Authorization': 'Bearer ' + getToken(),\n        },\n    body: JSON.stringify(todo),\n    })\n    .then(res => res.json())\n    .catch(err => console.log(err))\n}\n\nexport function deleteTodo(id) {\n  return fetch(`https://git.heroku.com/todos4teams.git/todos/delete/${id}?_method=DELETE`, {\n    method: 'POST',\n    headers: \n        {\n          'Content-Type': 'Application/json',\n          'Authorization': 'Bearer ' + getToken(),\n        },\n    })\n    .then(res => res.json())\n    .catch(err => console.log(err))\n}","/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/pages/CreateTodoPage/index.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/pages/TodoPage/index.js",["86"],"import TodoTable from '../../components/TodoTable';\nimport { Link } from 'react-router-dom';\nimport styles from './TodoPage.module.css';\n\nfunction TodoPage(props) {\n    return (\n        <div className={styles.TodoTable}>\n            <h3 className={styles.todoTitle}>Your List of Todos</h3>\n            <TodoTable \n                setUserTodos={props.setUserTodos}\n                todos={props.todos}\n                deleteHandler={props.deleteHandler}\n            />\n        </div>\n    )\n}\n\nexport default TodoPage;","/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/pages/EditTodoPage/index.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/components/TodoForm/index.js",[],"/Users/Jimenez/Documents/general-assembly/week-17/project-3/project-3-api-tutorial-client/project-3-api-turotial-client/src/components/TodoTable/index.js",["87","88","89"],"import { Link } from 'react-router-dom';\nimport { useForm } from 'react-hook-form';\nimport { useHistory } from 